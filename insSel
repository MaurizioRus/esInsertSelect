INSERT INTO ingredients (NAME , PRICE , GLUTEN_FREE)
VALUES
    ('eggs' , 1.99 , true),
    ('bread' , 2.49 , false),
    ('beef patty' , 3.99 , false),
    ('lettuce' , 1.99 , true),
    ('yogurt' , 1.49 , true);

SELECT NAME FROM ingredients;

SELECT NAME , PRICE FROM ingredients;

SELECT * FROM ingredients
ORDER BY NAME;

SELECT * FROM ingredients
ORDER BY GLUTEN_FREE DESC , NAME;